{
  "extends": [
    "airbnb"
  ],
  "env": {
    "jest": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "no-console": 2,
    "no-use-before-define": 0,
    "no-plusplus": 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "arrow-body-style": 0,
    "prefer-template": 0,
    "prefer-destructuring": 0
  }
}
